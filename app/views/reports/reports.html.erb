<div class="page-header">
<h1>List of Course Sections Attended by User</h1>
</div>
	
<table class = "table table-bordered table-hover">
	<tr>
		<th>User Email</th>
		<th>Course Section</th>
		<th>Attended</th>
	</tr>
<% @reports.each do |report| %>
	<tr>
		<td><%= report.user.name %></td>
		<td><%= report.coursesection_id %></td>
		<td><%= report.didattend %></td>
	</tr>
<% end %>
	
</table>

